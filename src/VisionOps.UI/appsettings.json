{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "System": "Warning"
    }
  },
  "VisionOps": {
    "Service": {
      "Name": "VisionOps",
      "DisplayName": "VisionOps Analytics Service",
      "CheckInterval": 5000
    },
    "Cameras": {
      "MaxCameras": 5,
      "FrameInterval": 3000,
      "KeyFrameInterval": 10000,
      "List": []
    },
    "AI": {
      "BatchSize": 16,
      "InferenceTimeout": 200,
      "ModelPath": "C:\\Program Files\\VisionOps\\models"
    },
    "Thermal": {
      "ThrottleTemp": 70,
      "CriticalTemp": 75
    },
    "Memory": {
      "MaxMemoryGB": 6,
      "RestartThresholdGB": 5.5
    },
    "Supabase": {
      "Url": "",
      "AnonKey": "",
      "SyncInterval": 30000
    },
    "AutoUpdate": {
      "Enabled": true,
      "CheckInterval": 21600000,
      "UpdateFeedUrl": "https://github.com/visionops/visionops/releases",
      "Channel": "stable"
    }
  }
}